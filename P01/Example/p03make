#-------------------------------------------------------------------------
# File p03make creates executable file p03.
#-------------------------------------------------------------------------
# Author: Thomas R. Turner 
# E-Mail: trturner@uco.edu
# Date:   November, 2011
#-------------------------------------------------------------------------
# Copyright November, 2011 by Thomas R. Turner.
# Do not reproduce without permission from Thomas R. Turner.
#-------------------------------------------------------------------------
#-------------------------------------------------------------------------
# Bind p03.o and F03.o
#-------------------------------------------------------------------------
p03:		p03.o F03.o
		g++ -o p03 p03.o F03.o -lm
#-------------------------------------------------------------------------
# Compile p03.cpp
#-------------------------------------------------------------------------
p03.o:		p03.cpp F03.h
		g++ -g -c p03.cpp
#-------------------------------------------------------------------------
# Compile F03.cpp
#--------------------------------------------------------------------------
F03.o:		F03.cpp F03.h
		g++ -g -c F03.cpp
