# Bind p03.o, F03.o
#-------------------------------------------------------------------------
p03:		p03.o F03.o
		g++ -o p03 p03.o F03.o -lm
#-------------------------------------------------------------------------
# Compile p03.cpp
#-------------------------------------------------------------------------
p03.o:		p03.cpp F03.h
		g++ -g -c p03.cpp
#-------------------------------------------------------------------------
# Compile F03.h
#-------------------------------------------------------------------------
F03.o:	F03.cpp F03.h
		g++ -g -c F03.cpp
